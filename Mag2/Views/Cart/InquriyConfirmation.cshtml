@model Mag2_Models.OrderHeader 


<div class="text-center">

    @if (Model != null)
    {
        <h1 class="text-info"> Order Create </h1>
        <p>Order #@Model.Id has been create successfuly!</p>
    }
    else
    {
        <h1 class="text-info">  Inquiry Confirmation  </h1>
        <p>  We have recieved your inquiry and someone from our team will contact you shortly!  </p>
    }

    <img src="/images/original.jpg" width="80%" />

</div>

